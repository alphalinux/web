Procedure gid_Procedure_Remove_Jre
	ProcName	 = "Remove_Jre";
	Styles		 = (STANDALONE, UNINSTALL, BEFORE_INST);
	Code		 = {Sub Remove_Jre

        Dim JrePath1 As String
        Dim JrePath2 As String
        Dim RemoveString1 As String
        Dim RemoveString2 As String

        JrePath1 = DirEntry(GetSetupEnv.DestPath, "program", "jre1.3.1_02")
        JrePath2 = DirEntry(GetSetupEnv.DestPath, "program", "j2re1_3_1_02")
        RemoveString1 = "rm -rf " & JrePath1
        RemoveString2 = "rm -rf " & JrePath2

        REM msgbox "Aufruf von: " & RemoveString

        shell(RemoveString1)
        shell(RemoveString2)

    End Sub


};
End

