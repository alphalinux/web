Procedure GID_PROCEDURE_GNOME_INSTALL
	ProcName	 = "GnomeInstall";
	Styles		 = (STANDALONE, WORKSTATION, INSTALL, REPAIRABLE, AFTER_INST);
	Code		 = {Sub GnomeInstall

                rem Searching for the installed applications. When installing GNOME, all files for all applications
                rem are installed. Therefore after the installation, the files, which are not necessary, have to
                rem be deleted.

                dim isWriter As Boolean
                dim isCalc As Boolean
                dim isDraw As Boolean
                dim isImpress As Boolean
                dim isMath As Boolean

                dim programPath As String
                dim searchFile As String
                dim officePath As String
                dim removePath As String
                dim removeCommand As String

                If GetsetupEnv.InstallMode = "IM_WORKSTATION" Then
                    programPath = DirEntry(GetSetupEnv.SourcePath, "program")
                Else
                    programPath = DirEntry(GetSetupEnv.DestPath, "program")
                EndIf

                rem looking for the file swriter

                searchFile = "swriter"

                If existsFile(programPath, searchFile) Then
                    isWriter = true
                Else
                    isWriter = false
                Endif

                searchFile = "scalc"

                If existsFile(programPath, searchFile) Then
                    isCalc = true
                Else
                    isCalc = false
                Endif

                searchFile = "sdraw"

                If existsFile(programPath, searchFile) Then
                    isDraw = true
                Else
                    isDraw = false
                Endif

                searchFile = "simpress"

                If existsFile(programPath, searchFile) Then
                    isImpress = true
                Else
                    isImpress = false
                Endif

                searchFile = "smath"

                If existsFile(programPath, searchFile) Then
                    isMath = true
                Else
                    isMath = false
                Endif

                rem And now the files can be deleted

                officePath = DirEntry( Environ("HOME"), ".gnome", "apps", "%PRODUCTNAME %PRODUCTVERSION" )

                If ( not isWriter ) Then
                    kill DirEntry( officePath, "textdoc.desktop" )
                    kill DirEntry( officePath, "webdoc.desktop" )
                    removePath = DirEntry( Environ("HOME"), ".gnome", "apps", "%PRODUCTNAME %PRODUCTVERSION", "auto pilots" )
                    removeCommand = "rm -rf " & "'" & removePath & "'"
                    shell(removeCommand)
                    removePath = DirEntry( Environ("HOME"), ".gnome", "apps", "%PRODUCTNAME %PRODUCTVERSION", "other" )
                    removeCommand = "rm -rf " & "'" & removePath & "'"
                    shell(removeCommand)

                Endif

                If ( not isCalc ) Then
                    kill DirEntry( officePath, "spreadsheet.desktop" )
                Endif

                If ( not isDraw ) Then
                    kill DirEntry( officePath, "drawing.desktop" )
                Endif

                If ( not isImpress ) Then
                    kill DirEntry( officePath, "presentation.desktop" )
                Endif

                REM and now the executable gnomeint can be started with all selected modules for registering the
                REM filetypes and extensions
                REM Example: gnomeint StarOffice 6.0 /export/home/staroffice6.0 writer calc draw impress

                dim gnomeFile as String
                dim gnomeFileCall as String
                dim progpath as String

                If GetsetupEnv.InstallMode = "IM_WORKSTATION" Then
                    gnomeFile = DirEntry(GetSetupEnv.SourcePath, "program", "gnomeint")
                    progpath = DirEntry(GetSetupEnv.SourcePath)
                Else
                    gnomeFile = DirEntry(GetSetupEnv.DestPath, "program", "gnomeint")
                    progpath = DirEntry(GetSetupEnv.DestPath)
                EndIf

                gnomeFileCall = gnomeFile & " " & "%PRODUCTNAME" & " " & "%PRODUCTVERSION" & " " & "'" & progpath & "'"

                If ( isWriter ) Then
                    gnomeFileCall = gnomeFileCall & " " & "writer"
                Endif

                If ( isCalc ) Then
                    gnomeFileCall = gnomeFileCall & " " & "calc"
                Endif

                If ( isDraw ) Then
                    gnomeFileCall = gnomeFileCall & " " & "draw"
                Endif

                If ( isImpress ) Then
                    gnomeFileCall = gnomeFileCall & " " & "impress"
                Endif

             rem    msgbox("Aufruf: " & gnomeFileCall)

                shell(gnomeFileCall)

             rem    SyncStartApplication(gnomeFile, param2, param3, progpath, language_parameter)

End Sub

