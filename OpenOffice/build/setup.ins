Procedure GID_PROCEDURE_KDE_INST_FOR_REDHAT
	ProcName	 = "KDEInstRedhat";
	Styles		 = (STANDALONE, WORKSTATION, INSTALL, REPAIRABLE, AFTER_INST);
	Code		 = {Sub KDEInstRedhat
	On Error goto SCRIPT_OBJECT_NOT_FOUND

    rem Searching for the installed applications. When installing KDE, all files for all applications
    rem are installed. Therefore after the installation, the files, which are not necessary, have to
    rem be deleted.

    dim isWriter As Boolean
    dim isCalc As Boolean
    dim isDraw As Boolean
    dim isImpress As Boolean
    dim isMath As Boolean

    dim programPath As String
    dim searchFile As String
    dim officePath As String
    dim removePath As String
    dim removeCommand As String

    If GetsetupEnv.InstallMode = "IM_WORKSTATION" Then
        programPath = DirEntry(GetSetupEnv.SourcePath, "program")
    Else
        programPath = DirEntry(GetSetupEnv.DestPath, "program")
    EndIf

    rem looking for the file swriter

    searchFile = "swriter"

    If existsFile(programPath, searchFile) Then
        isWriter = true
    Else
        isWriter = false
    Endif

    searchFile = "scalc"

    If existsFile(programPath, searchFile) Then
        isCalc = true
    Else
        isCalc = false
    Endif

    searchFile = "sdraw"

    If existsFile(programPath, searchFile) Then
        isDraw = true
    Else
        isDraw = false
    Endif

    searchFile = "simpress"

    If existsFile(programPath, searchFile) Then
        isImpress = true
    Else
        isImpress = false
    Endif

    searchFile = "smath"

    If existsFile(programPath, searchFile) Then
        isMath = true
    Else
        isMath = false
    Endif

    rem And now the files can be deleted

    officePath = DirEntry( Environ("HOME"), ".kde2", "share", "applnk", "%PRODUCTNAME %PRODUCTVERSION" )

    If ( not isWriter ) Then
        kill DirEntry( officePath, "textdoc.desktop" )
        kill DirEntry( officePath, "webdoc.desktop" )
        removePath = DirEntry( Environ("HOME"), ".kde2", "share", "applnk", "%PRODUCTNAME %PRODUCTVERSION", "auto pilots" )
        removeCommand = "rm -rf " & "'" & removePath & "'"
        shell(removeCommand)
        removePath = DirEntry( Environ("HOME"), ".kde2", "share", "applnk", "%PRODUCTNAME %PRODUCTVERSION", "other" )
        removeCommand = "rm -rf " & "'" & removePath & "'"
        shell(removeCommand)

    Endif

    If ( not isCalc ) Then
        kill DirEntry( officePath, "spreadsheet.desktop" )
    Endif

    If ( not isDraw ) Then
        kill DirEntry( officePath, "drawing.desktop" )
    Endif

    If ( not isImpress ) Then
        kill DirEntry( officePath, "presentation.desktop" )
    Endif

    Rem The rest of this script is necessary, because Redhat (and Redflag) uses the directory .kde instead of .kde2.
    Rem Therefore all file have to be copied from .kde2 to .kde if the file /etc/redhat-release or
    Rem the file /etc/redflag-release exists

    Dim isRedhat as boolean
    Dim currentDir as String
    Dim searchDir as String
    Dim sourceDir as String
    Dim destDir as String

REM    If Dir("/etc/redhat-release") = "redhat-release" Then
REM        isRedhat = true
REM        Rem msgbox "This is a Redhat system"
REM    Else
REM        isRedhat = false
REM        Rem msgbox "This is no Redhat system"
REM        Goto noRedhat
REM    Endif

REM       aFileName = Dir("/etc")
REM       Do while (aFileName <> "")
REM            If ((aFileName = "redhat-release") or (aFileName = "redflag-release")) Then
REM                  isRedhat = true
REM                  Goto dir_gefunden
REM            End If
REM            aFileName = Dir
REM       Loop
REM       isRedhat = false
REM       Goto noRedhat

    isRedhat = true
       dir_gefunden:

    Rem exists the directory structure for .kde?

    currentDir = DirEntry( Environ("HOME"))
    searchDir = ".kde"
    nextDir = DirEntry( Environ("HOME"), ".kde")

    If (Not existsDir(searchDir, currentDir)) Then
        mkdir nextDir
    Endif

    currentDir = nextDir
    searchDir = "share"
    nextDir = DirEntry( Environ("HOME"), ".kde", "share")

    If (Not existsDir(searchDir, currentDir)) Then
        mkdir nextDir
    Endif

    currentDir = nextDir
    searchDir = "applnk"
    nextDir = DirEntry( Environ("HOME"), ".kde", "share", "applnk")

    If (Not existsDir(searchDir, currentDir)) Then
        mkdir nextDir
    Endif

    currentDir = nextDir
    searchDir = "%PRODUCTNAME %PRODUCTVERSION"
    nextDir = DirEntry( Environ("HOME"), ".kde", "share", "applnk", "%PRODUCTNAME %PRODUCTVERSION")

    If (Not existsDir(searchDir, currentDir)) Then
        mkdir nextDir
    Endif

    currentDir = DirEntry( Environ("HOME"), ".kde", "share")
    searchDir = "mimelnk"
    nextDir = DirEntry( Environ("HOME"), ".kde", "share", "mimelnk")

    If (Not existsDir(searchDir, currentDir)) Then
        mkdir nextDir
    Endif

    currentDir = DirEntry( Environ("HOME"), ".kde", "share")
    searchDir = "icons"
    nextDir = DirEntry( Environ("HOME"), ".kde", "share", "icons")

    If (Not existsDir(searchDir, currentDir)) Then
        mkdir nextDir
    Endif

    Rem ... and now we can copy all files from .kde2 to .kde

    sourceDir = DirEntry( Environ("HOME"), ".kde2", "share", "applnk", "%PRODUCTNAME %PRODUCTVERSION")
    destDir = DirEntry( Environ("HOME"), ".kde", "share", "applnk")

    Rem msgbox "Copy from " & sourceDir & " to " & destDir

    shell( "cp -r " & "'" & sourceDir & "'" & " " & "'" & destDir & "'" )

    sourceDir = DirEntry( Environ("HOME"), ".kde2", "share", "mimelnk")
    destDir = DirEntry( Environ("HOME"), ".kde", "share")

    Rem msgbox "Copy from " & sourceDir & " to " & destDir

    shell( "cp -r " & "'" & sourceDir & "'" & " " & "'" & destDir & "'" )

    sourceDir = DirEntry( Environ("HOME"), ".kde2", "share", "icons")
    destDir = DirEntry( Environ("HOME"), ".kde", "share")

    Rem msgbox "Copy from " & sourceDir & " to " & destDir

    shell( "cp -r " & "'" & sourceDir & "'" & " " & "'" & destDir & "'" )

    noRedhat:

	SCRIPT_OBJECT_NOT_FOUND:

End Sub

Function existsFile(path as String, file as String) As Boolean
       dim localFile as String
       dim localPath as String
       dim aFileName as String

       localFile = file
       localPath = path

       aFileName = Dir(localPath)
       Do while (aFileName <> "")
            If (aFileName = localFile) Then
                  existsFile = true
                  rem msgbox "File " & localFile & " exists"
                  Goto file_exists
            End If
            aFileName = Dir
       Loop
       existsFile = false
       rem msgbox "File " & localFile & " does not exist"
       file_exists:
end function

Function existsDir(path as String, currentpath as String) As Boolean
       dim aDirName as String
       dim localPath as String
       localPath = currentpath
       aDirName = Dir(localPath,16)
       Do while (aDirName <> "")
            If (aDirName = path) Then
                  existsDir = true
                  Goto dir_gefunden
            End If
            aDirName = Dir
       Loop
       existsDir = false
       dir_gefunden:
End function



};
End

Procedure GID_PROCEDURE_KDE_DEINSTALL_FOR_REDHAT
	ProcName	 = "KDEDeinstall";
	Styles		 = (STANDALONE, WORKSTATION, UNINSTALL, BEFORE_INST);
	Code		 = {Sub KDEDeinstall

    On Error goto SCRIPT_OBJECT_NOT_FOUND

    Rem This script is necessary, because Redhat (or Redflag) uses the directory .kde instead of .kde2
    Rem Therefore all file have to be deleted from .kde if the file /etc/redhat-release or the file
    Rem /etc/redflag-release exists

    Dim isRedhat as boolean
    Dim currentDir as String
    Dim searchDir as String
    Dim sourceDir as String
    Dim destDir as String

        REM    If Dir("/etc/redhat-release") = "redhat-release" Then
        REM        isRedhat = true
        REM        Rem msgbox "This is a Redhat system"
        REM    Else
        REM        isRedhat = false
        REM        Rem msgbox "This is no Redhat system"
        REM        Goto noRedhat
        REM    Endif

    aFileName = Dir("/etc")
    Do while (aFileName <> "")
           If ((aFileName = "redhat-release") or (aFileName = "redflag-release")) Then
                isRedhat = true
                Goto file_gefunden
           End If
           aFileName = Dir
    Loop
    isRedhat = false
    Goto noRedhat
    file_gefunden:

    Rem exists the directory structure for .kde?

    currentDir = DirEntry( Environ("HOME"))
    searchDir = ".kde"

    If (Not existsDir(searchDir, currentDir)) Then
        Goto directory_removed
    Endif

    currentDir = DirEntry( Environ("HOME"), ".kde")
    searchDir = "share"

    If (Not existsDir(searchDir, currentDir)) Then
        Goto directory_removed
    Endif

    currentDir = DirEntry( Environ("HOME"), ".kde", "share")
    searchDir = "applnk"

    If (Not existsDir(searchDir, currentDir)) Then
        Goto directory_removed
    Endif

    currentDir = DirEntry( Environ("HOME"), ".kde", "share", "applnk")
    searchDir = "%PRODUCTNAME %PRODUCTVERSION"

    If (existsDir(searchDir, currentDir)) Then
        searchDir = DirEntry( Environ("HOME"), ".kde", "share", "applnk", "%PRODUCTNAME %PRODUCTVERSION")
        shell( "rm -rf " & "'" & searchDir & "'" )
    Endif

    directory_removed:

    noRedhat:

	SCRIPT_OBJECT_NOT_FOUND:

End Sub

Function existsDir(path as String, currentpath as String) As Boolean
       dim aDirName as String
       dim localPath as String
       localPath = currentpath
       aDirName = Dir(localPath,16)
       Do while (aDirName <> "")
            If (aDirName = path) Then
                  existsDir = true
                  Goto dir_gefunden
            End If
            aDirName = Dir
       Loop
       existsDir = false
       dir_gefunden:
End function



};
End

