                        strFileName = DirEntry(instpath, "share", "cde", SubNames(Count,2), "%PRODUCTNAME_%PRODUCTVERSION" & SubNames(Count,1))
                        kill strFileName
                        Count = Count + 1
                Loop

                other_path = DirEntry(instpath, "share", "cde", "other")
                auto_path = DirEntry(instpath, "share", "cde", "auto pilots")
                info_path = DirEntry(instpath, "share", "cde", "information and setup")

                RmDir other_path
                RmDir auto_path
                RmDir info_path

                dtint_call = "'" & dtint_file & "'" & " " & "-remove" & " " & "%PRODUCTNAME" & " " & "%PRODUCTVERSION"

             rem    msgbox("Aufruf: " & dtint_call)

                shell(dtint_call)

End Sub



};
End

Procedure GID_PROCEDURE_CDE_DTAPPINTEGRATE
	ProcName	 = "CDEDtappintegrate";
	Styles		 = (NETWORK, INSTALL, AFTER_INST);
	Code		 = {Sub CDEDtappintegrate

     REM First we have to execute the file "dtappintegrate"
     REM with the necessary parameters
     REM At the moment the script is started always, in future
     REM versions the user has to be asked for a system wide
     REM CDE integration.

     dim array_languages()
     dim userlanguage As Integer
     dim dtapp_file As String
     dim instpath As String
     dim language_paramter As String
     dim param2 As String
     dim param3 As String

     array_languages = GetSelectedLanguages
     userlanguage = array_languages(0)
     Select Case userlanguage
         Case 01
              language_parameter = "en"
         Case 03
             language_parameter = "pt"
         Case 07
             language_parameter = "ru"
         Case 30
             language_parameter = "el"
         Case 31
             language_parameter = "nl"
         Case 33
             language_parameter = "fr"
         Case 34
             language_parameter = "es"
         Case 39
             language_parameter = "it"
         Case 45
             language_parameter = "da"
         Case 46
             language_parameter = "sv"
         Case 48
             language_parameter = "pl"
         Case 49
             language_parameter = "de"
         Case 81
             language_parameter = "ja"
         Case 82
             language_parameter = "ko"
         Case 86
             language_parameter = "zh_CN"
         Case 88
             language_parameter = "zh_TW"
         Case 90
             language_parameter = "tr"
         Case 96
             language_parameter = "ar"
         Case 99
             language_parameter = "ISO_CODE"
         Case Else
             language_parameter = "en"
     End Select

     REM    the path is always the same, because the script is only
     REM    called in a network installation

     dtapp_file = DirEntry(GetSetupEnv.DestPath, "share", "cde", "dtappintegrate")
     instpath = DirEntry(GetSetupEnv.DestPath)

     param2 = "%PRODUCTNAME"
     param3 = "%PRODUCTVERSION"

     SyncStartApplication(dtapp_file, instpath, param2, param3, language_parameter)

     REM the script writes a file /share/cde/dtappintegrate.success if the system wide
     REM CDE integration was successful. Then we have to show the following MessageBox
     REM that the user has to restart the workspace manager.
     REM If dtappintegrate failed, it writes a file /share/cde/dtappintegrate.noroot or
     REM it cannot write any file. In these cases we do not write a MessageBox at the
     REM moment. In later versions we should give a hint, that the CDE integration failed.

     dim success_file As String
     dim noroot_file As String
     dim sharecde_path As String
     dim success_exists As Boolean
     dim noroot_exists As Boolean

     success_exists = false
     noroot_exists = false
     success_file = "dtappintegrate.success"
     noroot_file = "dtappintegrate.noroot"
     sharecde_path = DirEntry(GetSetupEnv.DestPath, "share", "cde")

     If existsFile(sharecde_path, success_file) Then
         success_exists = true
     Endif

     REM    If existsFile(sharecde_path, noroot_file) Then
     REM        noroot_exists = true
     REM    Endif

     REM    If ((Not success_exists) AND (Not noroot_exists)) Then
     REM        msbox "dtappintegrate was not completed"
     REM    Endif

     kill DirEntry(GetSetupEnv.DestPath, "share", "cde", "dtappintegrate.success")
     kill DirEntry(GetSetupEnv.DestPath, "share", "cde", "dtappintegrate.noroot")

     REM and finally we show the MessageBox
     REM if "dtappintegrate.success" exists

     dim UILanguage As Integer
     dim message1 As String
     dim message2 As String
     UILanguage = GetUILanguage

     If (success_exists) Then

        If ( Not IsResponsefileMode() ) Then

            Select Case UILanguage
                Case 01
                    SiMsgBox ("%PRODUCTNAME was added to the CDE configuration for the English language during the installation.")
                Case 03
                    SiMsgBox ("O %PRODUCTNAME foi adicionado  interface grfica CDE durante a instalao.")
                Case 07
                    message1 = cvtuilang( "      CDE  %PRODUCTNAME." )
                    SiMsgBox (message1)
                Case 30
                    SiMsgBox ("     %PRODUCTNAME  CDE-Frontpanel.")
                Case 31
                    SiMsgBox ("Gedurende de installatie werd %PRODUCTNAME toegevoegd aan het CDE-fontpanel.")
                Case 33
                    SiMsgBox ("Au cours de l'installation, %PRODUCTNAME a t ajout  la configuration du CDE pour le franais.")
                Case 34
                    SiMsgBox ("Durante la instalacin %PRODUCTNAME se aadi a la configuracin CDE para el idioma espaol.")
                Case 35
                    SiMsgBox ("%PRODUCTNAME was added to the CDE Frontpanel during the installation.")
                Case 37
                    SiMsgBox ("%PRODUCTNAME se aadi a la interfaz grfica (CDE) durante la instalacin.")
                Case 39
                    SiMsgBox ("Nel corso dell'installazione %PRODUCTNAME  stato aggiunto alla configurazione CDE per italiano.")
                Case 45
                    SiMsgBox ("Under installationen er %PRODUCTNAME blevet tilfjet til CDE-frontpanelet.")
                Case 46
                    SiMsgBox ("Under installationen lades %PRODUCTNAME till till CDE-konfigurationen fr sprket svenska.")
                Case 48
                    message1 = cvtuilang( "Podczas instalacji do interfejsu graficznego CDE doczono program %PRODUCTNAME." )
                    SiMsgBox (message1)
                Case 49
                    SiMsgBox ("Whrend der Installation wurde %PRODUCTNAME der CDE-Konfiguration fr die Sprache Deutsch hinzugefgt.")
                Case 81
                    SiMsgBox (" %PRODUCTNAME  CDE ")
                Case 82
                    SiMsgBox ("  %PRODUCTNAME()  CDE  .")
                Case 86
                    SiMsgBox (" %PRODUCTNAME  CDE ")
                Case 88
                    SiMsgBox (" %PRODUCTNAME  CDE ")
                Case 90
                    SiMsgBox ("Kurulum srasnda %PRODUCTNAME, CDE-Frontpanel'e eklendi.")
                Case 96
                    SiMsgBox ("  %PRODUCTNAME  CDE-Frontpanel   .")
                Case 99
                    SiMsgBox ("STR_BASIC_CDEINT_NETWORK#99")
                Case Else
                    SiMsgBox ("%PRODUCTNAME was added to the CDE configuration for the English language during the installation.")
            End Select

        Endif   REM ( Not IsResponsefileMode() )

     Endif  REM (success_exists)

End Sub

Function existsFile(path as String, file as String) As Boolean

       dim localFile as String
       dim localPath as String
       dim aFileName as String

       localFile = file
       localPath = path

       aFileName = Dir(localPath)
       Do while (aFileName <> "")
            If (aFileName = localFile) Then
                  existsFile = true
                  rem msgbox "File " & localFile & " exists"
                  Goto file_exists
            End If
            aFileName = Dir
       Loop
       existsFile = false
       rem msgbox "File " & localFile & " does not exist"
       file_exists:
end function



};
End

