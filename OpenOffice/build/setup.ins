                SubNames(6,2) = "auto pilots"
                SubNames(7,1) = "_fax"
                SubNames(7,2) = "auto pilots"
                SubNames(8,1) = "_agenda"
                SubNames(8,2) = "auto pilots"
                SubNames(9,1) = "_memo"
                SubNames(9,2) = "auto pilots"
                SubNames(10,1) = "_printeradmin"
                SubNames(10,2) = "information and setup"
                SubNames(11,1) = ""

                strFileName = DirEntry(GetSetupEnv.DestPath, "share", "cde", "information and setup", "%PRODUCTNAME on the Web")

                mkdir DirEntry(GetSetupEnv.DestPath, "share", "cde", "information and setup")

                lowercaseproductname = LCase("%PRODUCTNAME")

                strInputString = "http://www.sun.com/" & lowercaseproductname

                FileNrWrite = 1

                REM MsgBox("Anlegen der Datei: " & strFileName)
                Open strFileName For Output As FileNrWrite
                print #FileNrWrite strInputString
                Close #FileNrWrite

                Rem Create directories "other" + "auto pilots"
