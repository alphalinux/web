Procedure GID_PROCEDURE_GNOME_DEINSTALL
	ProcName	 = "GnomeDeinstall";
	Styles		 = (STANDALONE, WORKSTATION, UNINSTALL, BEFORE_INST);
	Code		 = {Sub GnomeDeinstall

                dim isWriter As Boolean
                dim isCalc As Boolean
                dim isDraw As Boolean
                dim isImpress As Boolean

                dim gnomeFile as String
                dim gnomeFileCall as String
                dim progpath as String
                dim programPath As String
                dim searchFile As String

                If GetsetupEnv.InstallMode = "IM_WORKSTATION" Then
                    gnomeFile = DirEntry(GetSetupEnv.SourcePath, "program", "gnomeint")
                    progpath = DirEntry(GetSetupEnv.SourcePath)
                    programPath = DirEntry(GetSetupEnv.SourcePath, "program")
                Else
                    gnomeFile = DirEntry(GetSetupEnv.DestPath, "program", "gnomeint")
                    progpath = DirEntry(GetSetupEnv.DestPath)
                    programPath = DirEntry(GetSetupEnv.DestPath, "program")
                EndIf

                rem looking for the file swriter

                searchFile = "swriter"

                If existsFile(programPath, searchFile) Then
                    isWriter = true
                Else
                    isWriter = false
                Endif

                searchFile = "scalc"

                If existsFile(programPath, searchFile) Then
                    isCalc = true
                Else
                    isCalc = false
                Endif

                searchFile = "sdraw"

                If existsFile(programPath, searchFile) Then
                    isDraw = true
                Else
                    isDraw = false
                Endif

                searchFile = "simpress"

                If existsFile(programPath, searchFile) Then
                    isImpress = true
                Else
                    isImpress = false
                Endif

                REM and now the executable gnomeint can be started with all selected modules for deregistering the
                REM filetypes and extensions
                REM Example: gnomeint -remove StarOffice 6.0 writer draw calc impress

                gnomeFileCall = gnomeFile & " " & "-remove" & " " & "%PRODUCTNAME" & " " & "%PRODUCTVERSION"

                If ( isWriter ) Then
                    gnomeFileCall = gnomeFileCall & " " & "writer"
                Endif

                If ( isCalc ) Then
                    gnomeFileCall = gnomeFileCall & " " & "calc"
                Endif

                If ( isDraw ) Then
                    gnomeFileCall = gnomeFileCall & " " & "draw"
                Endif

                If ( isImpress ) Then
                    gnomeFileCall = gnomeFileCall & " " & "impress"
                Endif

             rem    msgbox("Aufruf: " & gnomeFileCall)

                shell(gnomeFileCall)

             rem    SyncStartApplication(gnomeFile, param2, param3, progpath, language_parameter)

End Sub

Function existsFile(path as String, file as String) As Boolean

       dim localFile as String
       dim localPath as String
       dim aFileName as String

       localFile = file
       localPath = path

       aFileName = Dir(localPath)
       Do while (aFileName <> "")
            If (aFileName = localFile) Then
                  existsFile = true
                  rem msgbox "File " & localFile & " exists"
                  Goto file_exists
            End If
            aFileName = Dir
       Loop
       existsFile = false
       rem msgbox "File " & localFile & " does not exist"
       file_exists:
end function



};
End

