Procedure gid_Procedure_Copy_Iso_Res
	ProcName	 = "CopyIsoRes";
	Styles		 = (STANDALONE, NETWORK, INSTALL, AFTER_INST);
	Code		 = {Sub  CopyIsoRes
    rem is, 09.10.2000
    rem Renames the ooo-res into iso-res for OpenOffice
    dim resourcePath As String
    dim intVclNr as integer
    dim i as integer
    dim countrynumber as integer
    dim aFileName as String
    dim quelldatei as String
    dim zieldatei as String
    dim isoresource as String
    dim newisoresource as String

    countrynumber = 19

    dim countrystrings(countrynumber) as String

    countrystrings(0) = "01"
    countrystrings(1) = "03"
    countrystrings(2) = "07"
    countrystrings(3) = "31"
    countrystrings(4) = "33"
    countrystrings(5) = "34"
    countrystrings(6) = "35"
    countrystrings(7) = "37"
    countrystrings(8) = "39"
    countrystrings(9) = "45"
    countrystrings(10) = "46"
    countrystrings(11) = "48"
    countrystrings(12) = "49"
    countrystrings(13) = "81"
    countrystrings(14) = "82"
    countrystrings(15) = "86"
    countrystrings(16) = "88"
    countrystrings(17) = "90"
    countrystrings(18) = "96"
    countrystrings(19) = "99"

    resourcePath = DirEntry( GetSetupEnv.DestPath, "program", "resource" )
	intVclNr = GetMajor()

    for i=0 to countrynumber

    isoresource = "ooo" & intVclNr & countrystrings(i) & ".res"
    newisoresource = "iso" & intVclNr & countrystrings(i) & ".res"
    aFileName = Dir(resourcePath,0)
       Do while (aFileName <> "")
         If (aFileName = isoresource) Then
            quelldatei = DirEntry(resourcePath, isoresource)
            zieldatei = DirEntry(resourcePath, newisoresource)
            FileCopy (quelldatei, zieldatei)
         End If
         aFileName = Dir
       Loop

    next i

    End Sub


};
End

Procedure gid_Procedure_Patch_Font_Dir
	ProcName	 = "PatchFontsDir";
	Styles		 = (STANDALONE, NETWORK, INSTALL, UNINSTALL, AFTER_INST);
	Code		 = {Sub PatchFontsDir

        Dim oldpath as String
        Dim fontspath as String
        Dim anzahl as Integer
        Dim maxFonts as Integer
        Dim numberOfTtfs as Integer
        Dim legalFonts as Integer
        Dim i as Integer
        Dim aFileName as String
        Dim aFontName as String
        Dim strLine as String
        Dim fileNrRead as Integer
        Dim fileNrWrite as Integer
        Dim strNumberOfFonts as String
        Dim isFirstInstallation as Boolean

        rem oldpath = CurDir

        fontspath = DirEntry(GetSetupEnv.DestPath, "share", "fonts", "truetype")

        chdir fontspath

        rem if fonts.dir does not exits, this is a complete deinstallation
        rem and this script can be ignored. It has to be executed, when some
        rem modules are deselected.

        aFileName = Dir(fontspath, 0)
        Do while (aFileName <> "")
            If (aFileName = "fonts.dir") Then
                  REM    msgbox "This is no complete deinstallation"
                  Goto fonts_dir_gefunden
            End If
            aFileName = Dir
        Loop
        REM    msgbox "This is a complete deinstallation"
        Goto fonts_dir_not_found
        fonts_dir_gefunden:

        rem if fonts_dir.global does not exist (original installation)
        rem we have to copy fonts.dir to fonts.dir.global

        isFirstInstallation = true

        aFileName = Dir(fontspath, 0)
        Do while (aFileName <> "")
            If (aFileName = "fonts_dir.global") Then
                  isFirstInstallation = false
                  REM    msgbox "This is not the first installation"
                  Goto file_gefunden
            End If
            aFileName = Dir
        Loop
        REM    msgbox "This is the first installation"
        file_gefunden:

        rem rename the fonts.dir

        fontsDirFile = DirEntry(fontspath, "fonts.dir")
        fontsDirGlobalFile = DirEntry(fontspath, "fonts_dir.global")

        If (isFirstInstallation = true) Then
            FileCopy(fontsDirFile, fontsDirGlobalFile)
        EndIf

        Kill fontsDirFile

        rem collecting all ttf files

        maxFonts = 80

        Dim arrayFonts(maxFonts) as String

        i = 0

        aFileName = Dir(fontspath,0)
        Do while (aFileName <> "")
                If (InStr(aFileName,"ttf")) Then
                        arrayFonts(i) = aFileName
                        i = i + 1
                EndIf
                aFileName = Dir
        Loop

        arrayFonts(i) = "EndOfFonts"

        numberOfTtfs = i

        rem msgbox("Found " & numberOfTtfs & " fonts!")

        rem attention: not all files including ttf must be legal fonts!

        Dim arrayLegalFonts(maxFonts) as String

        rem reading the fonts_dir.global
        rem writing the fonts.dir

        fileNrRead = 1
        fileNrWrite = 2

        Open fontsDirGlobalFile For Input As FileNrRead
        Open fontsDirFile For Output As FileNrWrite

        rem First line of fonts.dir includes the number of fonts

        legalFonts = 0

        arrayLegalFonts(0) = "0"

        Do While Not EOF(FileNrRead)
            Line Input #FileNrRead strLine

            rem iterating over the Fontsarray
            rem counting the exact number of found fonts

            i = 0

            For i = 0 To numberOfTtfs

                aFontName = arrayFonts(i)

                If (InStr(strLine, aFontName)) Then
                        legalFonts = legalFonts + 1
                        arrayLegalFonts(legalFonts) = strLine
                        Exit For
                EndIf

            Next

        Loop

        strNumberOfFonts = legalFonts

        arrayLegalFonts(0) = strNumberOfFonts

        For i = 0 To legalFonts

           print #FileNrWrite arrayLegalFonts(i)

        Next

        Close #1,#2

        rem Kill fontsDirGlobalFile

        fonts_dir_not_found:

        rem chdir oldpath

    End Sub


};
End

