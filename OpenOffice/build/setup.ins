Procedure gid_Procedure_Kill_Dirs
	ProcName	 = "Kill_Dirs";
	Styles		 = (STANDALONE, NETWORK, UNINSTALL, BEFORE_INST);
	Code		 = {Sub Kill_Dirs
	Dim strPath  As String
	Dim strDirName  As String
	Dim strSubDirName(26) As String
	Dim strFileName As String
	Dim strFullFileName As String
	Dim i As Integer
	Dim k As Integer


	strPath = DirEntry(GetSetupEnv.DestPath, "share", "wordbook" )
	i = 0

	Rem Hole SubDirectorys
	strDirName = Dir( strPath, ATTR_DIRECTORY )

	Do While strDirName <> ""
      	  If (strDirName <> ".")  And (strDirName <> "..") Then
	       	rem MsgBox("strDirName = " & strDirName)
		strSubDirName(i) = DirEntry( strPath, strDirName )
		i = i + 1
		EndIf
            strDirName = Dir
  	 Loop

	Rem Alle Files in den SubDirs lschen
	For k=0 to i-1
       		strFileName = Dir( strSubDirName(k), ATTR_NORMAL )
		Do While strFileName <> ""
      		  If (strFileName <> ".")  And (strFileName <> "..") Then
			strFullFileName = DirEntry( strSubDirName(k), strFileName )
	       		rem MsgBox("strFullFileName = " & strFullFileName)
			kill strFullFileName
	       		EndIf
           	 strFileName = Dir
  	 	Loop
	 next k

        End Sub


};
End

