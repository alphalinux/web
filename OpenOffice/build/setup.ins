	Code		 = {Sub CDEHelper

                rem Anlegen von Dateien

                dim FileNrWrite as Integer
                dim strFileName as String
                dim strInputString as String
                dim SubNames(11,2) as String
                dim lowercaseproductname as String
                dim Count as Integer

                Rem (x,1)=ending of filename, (x,2)=subdirectory of share/cde
